{
	// Place your snippets for dart here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"bold":{
		"prefix": "bold",
		"body": "fontWeight: FontWeight.bold,"
	},
	"normal":{
		"prefix": "normal",
		"body": "fontWeight: FontWeight.normal,"
	},
	"style":{
		"prefix": "style",
		"body": " style: TextStyle(${1}),"
	},
	"field":{
		"prefix": "field",
		"body": [
			"final TextEditingController ${1:_email}Controller=TextEditingController();",
			"final FocusNode ${1:_email}FocusNode=FocusNode();"
		]
	},
	"form":{
		"prefix": "form",
		"body": [
			"final GlobalKey<FormState> _formKey = GlobalKey<FormState>();"
			
		]
	},
	"routeName":{
		"prefix": "routename",
		"body": [
			"static const routeName = '${1}_screen';"
			
		]
	},
	"focus": {
		"prefix": "nextfocus",
		"body": "FocusScope.of(context).nextFocus();"
	  },
	  "oneditcomplete": {
		"prefix": "oneditcomplete",
		"body": "onEditingComplete:()=>FocusScope.of(context).nextFocus(),"
	  },
	  "provider": {
		"prefix": "offalse",
		"body": "Provider.of<${1:AppState}>(context, listen: false).$2"
	  },
	  "routename": {
		"prefix": "routename",
		"body": "static const routeName = \"$1\";"
	  }

}